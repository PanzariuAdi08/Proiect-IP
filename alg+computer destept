{
        setcolor(WHITE);
        settextjustify(CENTER_TEXT,CENTER_TEXT);
        settextstyle(SANS_SERIF_FONT,HORIZ_DIR,1);
        outtextxy(latime*0.5,inaltime*0.10,"                ALEGETI O PIESA                ");
}
